{
  "cli": {
    "version": ">= 5.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "preview-standard": {
      "env": {
        "EXPO_PUBLIC_DEBUG_UNDO": "false"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview-debug-undo": {
      "env": {
        "EXPO_PUBLIC_DEBUG_UNDO": "true"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}